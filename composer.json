{
    "name": "mmerlijn/helper-time-array",
    "description": "TimeArray helper",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Menno",
            "email": "menno@mmerlijn.nl"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.x-dev"
    },
    "autoload": {
        "psr-4": {
            "mmerlijn\\helperTimeArray\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "mmerlijn\\helperTimeArray\\tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-f": "vendor/bin/phpunit --filter"
    },
    "config": {
        "sort-packages": true
    }
}
